{% extends "vxmlLayout.vxml" %}
{% block content %}
{% block result %}
<menu dtmf="true">
<block>
<prompt>

<audio src="{{ interfaceAudioDir ~ messageAudio }}"/>
{% for option in results %}
    {% for suboption in option %}
    <audio src="{{ suboption }}"/>
    {% endfor %}
{% endfor %}
</prompt>
<goto next='{{ redirect }}' />
</block>

</menu>
{% endblock %}
{% endblock %}
